{
  "name": "three-jarvis",
  "version": "1.1.16",
  "description": "A threejs development aid",
  "repository": "git@github.com:MFinnnne/three-jarvis.git",
  "author": "MFine <1737374338@qq.com>",
  "license": "GPL-3.0 License",
  "private": false,
  "main": "./dist/three-jarvis.cjs.js",
  "module": "./dist/three-jarvis.esm.js",
  "types": "./dist/index.d.ts",
  "prettier": "../../prettier.config.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "build": "run-s clean build:type prod",
    "build:watch": "run-s clean build:type dev",
    "build:ts": "run-p build:ts:*",
    "build:ts:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:ts:es6": "tsc --project ./tsconfig.es6.json",
    "build:type": "tsc",
    "start": "run-s build watch",
    "watch": "run-p watch:*",
    "watch:ts": "onchange --initial --kill \"src/**/*.ts\" -- pnpm run build:ts",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development rollup  --bundleConfigAsCjs -c ./scripts/rollup.config.dev.js ",
    "prod": "cross-env NODE_ENV=product rollup  --bundleConfigAsCjs -c ./scripts/rollup.config.prod.js ",
    "test": "jest"
  },
  "pre-commit": [
    "lint:fix",
    "format"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-alias": "~4.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "~15.0.1",
    "@rollup/plugin-replace": "~5.0.2",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "~18.14.2",
    "@types/three": "^0.138.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "nanoid": "^3.3.4",
    "npm-run-all": "~4.1.5",
    "postcss": "^8.4.14",
    "pre-commit": "^1.2.2",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "~3.17.3",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "~0.34.1",
    "sass": "^1.52.1",
    "three": "^0.138.3",
    "ts-node": "~10.9.1",
    "tslib": "^2.4.0",
    "typescript": "4.2.3",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.1.1",
    "@right-menu/core": "0.0.7",
    "@tweakpane/plugin-essentials": "^0.1.5",
    "@types/toastify-js": "^1.11.1",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.2",
    "million": "^1.0.9",
    "my-tweakpane": "1.0.0",
    "nanoevents": "^6.0.2",
    "onchange": "~7.1.0",
    "pino": "^8.8.0",
    "rollup-plugin-scss": "~4.0.0",
    "toastify-js": "^1.12.0"
  },
  "peerDependencies": {
    "three": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
